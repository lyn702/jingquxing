page {
  width: 100%;
  height: 100%;
  background: #fff;
}

.register_phone {
  display: flex;
  margin-top: 40rpx;
  margin-left: 20rpx;
  width: 710rpx;
  background: #f0f0f0;
}

.register_phone_lable {
  margin: 10rpx;
  width: 160rpx;
  font-size: 12pt;
}

.register_phone_input {
  width: 450rpx;
  margin: 10rpx;
  font-size: 12pt;
}

.register_code {
  display: flex;
  width: 710rpx;
  margin-top: 20rpx;
  margin-left: 20rpx;
  background: #f0f0f0;
}

.register_code_lable {
  margin: 10rpx;
  width: 160rpx;
  font-size: 12pt;
}

.register_code_input {
  margin: 10rpx;
  width: 340rpx;
  font-size: 12pt;
}

.register_code_butt {
  width: 150rpx;
  font-size: 12pt;
  text-align: center;
  border: none;
  border-radius: 5px;
}

.register_reg {
  margin: 10rpx;
  margin-left: 5%;
  width: 90%;
}

.register_reg_butt {
  font-size: 12pt;
  width: 100%;
  border: none;
  border-radius: 4px;
  margin-top: 40rpx;
}

.register_reg_checkbox {
  width: 100%;
  margin: 10px;
}

.getphone {
  position: absolute;
  top: 560rpx;
  left: 5%;
  width: 90%;
  margin: 20px auto;
  text-align: center;
}

.getphone>button {
  margin-top: 20px;
}

.logo {
  display: absolute;
  margin-left: 150rpx;
  margin-top: 20px;
}

.logo>image {
  width: 450rpx;
  height: 450rpx;
}
